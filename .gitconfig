[alias]
  tags = tag -l
  branches = branch -a
  remotes = remote -v
  hist = log --graph --pretty=format:'%Cred%h%Creset %s%C(yellow)%d%Creset %Cgreen(%cr)%Creset [%an]' --abbrev-commit --date=relative
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[branch "master"]
  remote = origin
  merge = refs/heads/master
[push]
  default = simple
[credential]
  helper = osxkeychain
